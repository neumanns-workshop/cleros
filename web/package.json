{
  "name": "cleros",
  "version": "2.0.0",
  "private": true,
  "homepage": "/cleros/",
  "dependencies": {
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.15.5",
    "@mui/material": "^5.15.5",
    "@tensorflow-models/universal-sentence-encoder": "^1.3.3",
    "@tensorflow/tfjs": "^4.22.0",
    "@testing-library/jest-dom": "^5.17.0",
    "@types/node": "^18.19.17",
    "@types/react": "^18.2.57",
    "@types/react-dom": "^18.2.19",
    "axios": "^1.8.4",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "html2canvas": "^1.4.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "tinycolor2": "^1.6.0",
    "typescript": "^4.9.5"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "build:prod": "react-scripts build",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "analyze:full": "node scripts/analyze-bundle.js",
    "serve": "serve -s build",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint src --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,scss}\"",
    "find-unused": "node scripts/find-unused.js",
    "pre-deploy": "node scripts/pre-deploy.js",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "chalk": "^4.1.2",
    "eslint-config-react-app": "^7.0.1",
    "prettier": "^3.2.5",
    "serve": "^14.2.1",
    "source-map-explorer": "^2.5.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jneumann/cleros.git"
  }
